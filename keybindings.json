// Place your key bindings in this file to override the defaultsauto[]
[
    {
        "key": "cmd+j",
        "command": "workbench.action.focusNextGroup"
    },
    {
        "key": "cmd+shift+j",
        "command": "workbench.action.focusPreviousGroup"
    },
    {
        "key": "cmd+6",
        "command": "workbench.actions.view.problems"
    },
    {
        "key": "cmd+7",
        "command": "workbench.action.output.toggleOutput"
    },
    {
        "key": "cmd+j",
        "command": "workbench.action.focusActiveEditorGroup",
        "when": "terminalFocus",
    },
    {
        "key": "alt+up",
        "command": "cursorMove",
        "args": {
            "to": "up",
            "by": "line",
            "value": 15
        },
        "when": "editorTextFocus"
    },
    {
        "key": "alt+down",
        "command": "cursorMove",
        "args": {
            "to": "down",
            "by": "line",
            "value": 15
        },
        "when": "editorTextFocus"
    },
    {
        "key": "alt+cmd+r",
        "command": "-toggleSearchEditorRegex",
        "when": "inSearchEditor && searchInputBoxFocus"
    },
    {
        "key": "cmd+r cmd+k",
        "command": "-editor.action.defineKeybinding",
        "when": "resource == 'vscode-userdata:/Users/ryo/Library/Application%20Support/Cursor/User/keybindings.json'"
    },
    {
        "key": "cmd+k cmd+n",
        "command": "-git.unstageSelectedRanges",
        "when": "editorTextFocus && isInDiffEditor && isInDiffRightEditor && !operationInProgress && resourceScheme == 'git'"
    },
    {
        "key": "cmd+8",
        "command": "-workbench.action.focusEighthEditorGroup"
    },
    {
        "key": "cmd+9",
        "command": "workbench.action.terminal.toggleTerminal",
        "when": "terminal.active"
    },
    {
        "key": "ctrl+`",
        "command": "-workbench.action.terminal.toggleTerminal",
        "when": "terminal.active"
    },
    {
        "key": "cmd+9",
        "command": "-workbench.action.lastEditorInGroup"
    },
    {
        "key": "cmd+8",
        "command": "workbench.debug.action.toggleRepl",
        "when": "workbench.panel.repl.view.active"
    },
    {
        "key": "shift+alt+cmd+y",
        "command": "-workbench.debug.action.toggleRepl",
        "when": "workbench.panel.repl.view.active"
    },
    {
        "key": "cmd+1",
        "command": "workbench.view.explorer",
        "when": "viewContainer.workbench.view.explorer.enabled"
    },
    {
        "key": "shift+cmd+e",
        "command": "-workbench.view.explorer",
        "when": "viewContainer.workbench.view.explorer.enabled"
    },
    {
        "key": "cmd+2",
        "command": "workbench.view.search",
        "when": "workbench.view.search.active && neverMatch =~ /doesNotMatch/"
    },
    {
        "key": "shift+cmd+f",
        "command": "-workbench.view.search",
        "when": "workbench.view.search.active && neverMatch =~ /doesNotMatch/"
    },
    {
        "key": "cmd+3",
        "command": "workbench.view.scm",
        "when": "workbench.scm.active"
    },
    {
        "key": "ctrl+shift+g",
        "command": "-workbench.view.scm",
        "when": "workbench.scm.active"
    },
    {
        "key": "cmd+5",
        "command": "workbench.view.extensions",
        "when": "viewContainer.workbench.view.extensions.enabled"
    },
    {
        "key": "shift+cmd+x",
        "command": "-workbench.view.extensions",
        "when": "viewContainer.workbench.view.extensions.enabled"
    },
    {
        "key": "cmd+4",
        "command": "workbench.view.backgroundAgent",
        "when": "viewContainer.workbench.view.backgroundAgent.enabled"
    },
    {
        "key": "shift+cmd+b",
        "command": "-workbench.view.backgroundAgent",
        "when": "viewContainer.workbench.view.backgroundAgent.enabled"
    },
    {
        "key": "cmd+4",
        "command": "-workbench.action.focusFourthEditorGroup"
    },
    {
        "key": "cmd+3",
        "command": "-workbench.action.focusThirdEditorGroup"
    },
    {
        "key": "cmd+2",
        "command": "-workbench.action.focusSecondEditorGroup"
    },
    {
        "key": "cmd+2",
        "command": "workbench.action.findInFiles"
    },
    {
        "key": "shift+cmd+f",
        "command": "-workbench.action.findInFiles"
    },
    {
        "key": "cmd+0",
        "command": "workbench.action.toggleSidebarVisibility"
    },
    {
        "key": "cmd+b",
        "command": "-workbench.action.toggleSidebarVisibility"
    },
    {
        "key": "alt+cmd+0",
        "command": "workbench.action.toggleAuxiliaryBar"
    },
    {
        "key": "alt+cmd+b",
        "command": "-workbench.action.toggleAuxiliaryBar"
    },
    {
        "key": "shift+cmd+e",
        "command": "composer.showBackgroundAgentHistory",
        "when": "backgroundComposerEnabled || showBackgroundAgentHistoryAction"
    },
    {
        "key": "cmd+e",
        "command": "-composer.showBackgroundAgentHistory",
        "when": "backgroundComposerEnabled || showBackgroundAgentHistoryAction"
    },
    {
        "key": "shift+cmd+k",
        "command": "-aipopup.action.modal.generate",
        "when": "editorFocus && !composerBarIsVisible"
    },
    {
        "key": "cmd+i",
        "command": "aipopup.action.modal.generate",
        "when": "editorFocus && !composerBarIsVisible"
    },
    {
        "key": "cmd+k",
        "command": "-aipopup.action.modal.generate",
        "when": "editorFocus && !composerBarIsVisible"
    },
    {
        "key": "cmd+e",
        "command": "composer.startComposerPrompt"
    },
    {
        "key": "cmd+i",
        "command": "-composer.startComposerPrompt"
    },
    {
        "key": "shift+cmd+i",
        "command": "-composer.newAgentChat"
    },
    {
        "key": "cmd+r cmd+g",
        "command": "-aiFeedback.action.open"
    },
    {
        "key": "shift+cmd+n",
        "command": "explorer.newFolder",
        "when": "explorerResourceMoveableToTrash && filesExplorerFocus && foldersViewVisible && !inputFocus"
    },
    {
        "key": "alt+cmd+9",
        "command": "workbench.action.toggleMaximizedPanel"
    },
    {
        "key": "shift+cmd+0",
        "command": "fontshortcuts.resetEditorFontSize",
        "when": "editorFocus"
    },
    {
        "key": "cmd+0",
        "command": "-fontshortcuts.resetEditorFontSize",
        "when": "editorFocus"
    },
    {
        "key": "shift+cmd+0",
        "command": "fontshortcuts.resetTerminalFontSize",
        "when": "terminalFocus"
    },
    {
        "key": "cmd+0",
        "command": "-fontshortcuts.resetTerminalFontSize",
        "when": "terminalFocus"
    },
    {
        "key": "alt+w",
        "command": "editor.action.toggleWordWrap"
    },
    {
        "key": "alt+z",
        "command": "-editor.action.toggleWordWrap"
    }
]
